import sympy
def primelist():
  return(list(sympy.primerange(1000, 10000)))
def dif3330():
  li2 = []
  li3 =[]
  li = primelist()
  for i in range(len(primelist())-1) :
    for j in range(1,len(primelist())-1):
      if li[i]-li[j] == 3330 and set[int(d) for d in str(li[i])]) == set([int(d) for d in str(li[j])]) :
        li2.append(li[i])
        li2.append(li[j])
  print(set(li2))
  
dif3330()
